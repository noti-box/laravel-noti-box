{
    "name": "noti-box/laravel-noti-box",
    "description": "This is my package laravel-noti-box",
    "keywords": [
        "noti-box",
        "laravel",
        "laravel-noti-box"
    ],
    "homepage": "https://github.com/noti-box/laravel-noti-box",
    "license": "MIT",
    "authors": [
        {
            "name": "James Mills",
            "email": "james@mynotibox.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.8",
        "orchestra/testbench": "^8.8",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "NotiBox\\NotiBox\\": "src/",
            "NotiBox\\NotiBox\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotiBox\\NotiBox\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-noti-box --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotiBox\\NotiBox\\NotiBoxServiceProvider"
            ],
            "aliases": {
                "NotiBox": "NotiBox\\NotiBox\\Facades\\NotiBox"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}